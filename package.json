{
  "name": "nestjs-starter",
  "version": "1.0.0",
  "description": "NestJS Backend Starter Template with Authentication, Authorization, Caching and Database Migration",
  "author": "@jirateep12z",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "nest start",
    "build": "nest build",
    "lint": "eslint \"**/*.{js,ts,md}\" --fix",
    "format": "prettier --write \"**/*.{js,ts,md}\"",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "bun dist/main",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "bun typeorm -- migration:generate -d src/config/database/data-source.ts",
    "migration:create": "bun typeorm -- migration:create",
    "migration:run": "bun typeorm -- migration:run -d src/config/database/data-source.ts",
    "migration:revert": "bun typeorm -- migration:revert -d src/config/database/data-source.ts",
    "migration:fresh": "bun schema:drop && bun migration:run",
    "migration:fresh:seed": "bun schema:drop && bun migration:run && bun seed",
    "schema:sync": "bun typeorm -- schema:sync -d src/config/database/data-source.ts",
    "schema:drop": "bun typeorm -- schema:drop -d src/config/database/data-source.ts",
    "seed": "bun ts-node src/database/seeds/index.ts"
  },
  "dependencies": {
    "@fastify/compress": "^8.1.0",
    "@fastify/cors": "^10.0.1",
    "@fastify/helmet": "^12.0.1",
    "@fastify/multipart": "^9.2.1",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/static": "^8.2.0",
    "@nestjs/cache-manager": "2.2.2",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-fastify": "^11.1.6",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^8.0.7",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "@scalar/nestjs-api-reference": "^1.0.4",
    "@types/multer": "^2.0.0",
    "archiver": "^7.0.1",
    "bcrypt": "^5.1.1",
    "cache-manager": "4.1.0",
    "cache-manager-ioredis": "2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "fastify": "^5.6.1",
    "ioredis": "^5.8.1",
    "ip-cidr": "^4.0.2",
    "mysql2": "^3.15.2",
    "nodemailer": "^7.0.9",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "speakeasy": "^2.0.0",
    "typeorm": "^0.3.27",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.8",
    "@nestjs/testing": "^11.1.6",
    "@types/bcrypt": "^5.0.2",
    "@types/cache-manager": "^4.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/nodemailer": "^7.0.2",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0"
  }
}
